["value"] = "-- Fracture is your primary builder for Fury and Soul Fragments.\n-- Cast it when you have less than 4 Soul Fragments and less than 135 Fury.\n-- In  Metamorphosis, cast it when you have less than 3 Soul Fragments and less than 115 Fury.\n\n\n\nfunction(states)\n    local power = states[1].power or 0\n    local stack = states[2].stacks or 0\n    local metaActive = states[3].show\n    \n    if metaActive then -- Meta is activated\n        if (power >= 115) or (stack >= 3) then\n            if (power >= 30) then\n                return true -- We cast Soul Cleave to dump Fury / Soul Fragments\n            end\n        end\n    else\n        if (power >= 135) or (stack >= 4) then\n            if (power >= 30) then\n                return true -- We cast Soul Cleave to dump Fury / Soul Fragments\n            end\n        end\n    end\n    return false -- We cast Fracture\nend",
