["[Shortwire] TWW - PVE Stats"] = {
["outline"] = "OUTLINE",
["xOffset"] = 0,
["displayText"] = "%c",
["customText"] = "function()\n    \n    \n    -- [[ ################################ DISCLAIMER! ################################ ]]\n    --\n    --     ** SIMMING YOUR CHARACTER WILL ALWAYS GIVE YOU THE BEST RESULTS. **\n    --\n    --     NOTE: All data is gathered from archon.gg and wowhead to supplement.\n    --\n    --     EXAMPLE: As of TWW S1, Frost DK both Raids and Mythic+ as Deathbring.\n    --     Archon.gg only shows the stat prio of that one spec. I have used wowhead\n    --     to supplement the stat prio for those who wish to play Riders of the Apocalypse.\n    --\n    --\n    --     https://www.archon.gg/wow/articles/help/archon-disclaimers-and-faq\n    --\n    --     LAST UPDATED: 2/7/2025 - Archon.gg // wowhead.com\n    --\n    -- [[ ############################################################################# ]]\n    \n    \n    local _,myclass,_= UnitClass(\"player\");\n    local currentSpec = GetSpecialization()\n    local currentSpecName = currentSpec and select(2, GetSpecializationInfo(currentSpec)) or \"None\"\n    local spec = (string.upper(currentSpecName))\n    local heroId = C_ClassTalents.GetActiveHeroTalentSpec()\n    local config = aura_env.config\n    if config.sui_playerstats_custom_enable then\n        return config.sui_playerstats_custom_prio\n    end\n    \n    local t = \"|c00FFFF00Stat Prio: |r\"\n    local r = \"|c00FFFFFF[R] |r\"\n    local m = \"|c00FFFFFF[M] |r\"\n    local b = \"|c00FFFFFF[R/M] |r\"\n    local s = \"|c00FFFFFF[S] |r\"\n    local e = \"|c00C41E3AERROR: This Spec does not support this hero talent.|r\"\n    \n    if config.sui_playerstats_debug then\n        print(\"\\n\\n\\n\\n\\n\")\n        print(\"####################\")        \n        print(\"[Shortwire] TWW - PVE Stats DEBUGGER\")        \n        print(\"CLASS: \" .. myclass)\n        print(\"SPEC: \" .. spec)        \n        if heroId ~= nil then\n            print(\"HEROID: \" .. heroId)\n        end\n        print(\"####################\")        \n    end\n    \n    local stats = -- [1] = Raid Stats, [2] = Mythic+ stats. Adjust accordingly.\n    {\n        DEATHKNIGHT = \n        {\n            FROST = \n            {\n                [31] = e, -- San'layn\n                [32] = t .. s .. \"ILVL>CRIT>MAST>HASTE>VERS\", -- Riders of the Apocalypse (wowhead.com / supplement)\n                [33] = t .. b .. \"ILVL>CRIT>MAST>HASTE>VERS\", -- Deathbringer (archon.gg / raid & m+)\n            },\n            UNHOLY = \n            {\n                [31] = t .. s .. \"ILVL>HASTE>MAST>VERS>CRIT\", -- San'layn (wowhead.com / supplement)\n                [32] = t .. b .. \"ILVL>HASTE>MAST>CRIT>VERS\", -- Riders of the Apocalypse (archon.gg / raid & m+)\n                [33] = e, -- Deathbringer\n            },\n            BLOOD = \n            {\n                [31] = t .. r .. \"ILVL>HASTE>CRIT>MAST>VERS\", -- San'layn (archon.gg / raid)\n                [32] = e, -- Riders of the Apocalypse\n                [33] = t .. m .. \"ILVL>MAST>HASTE>VERS>CRIT\", -- Deathbringer (archon.gg / m+)\n            },\n        },\n        DEMONHUNTER = \n        {\n            HAVOC = \n            {\n                [34] = t .. b .. \"ILVL>CRIT>MAST>HASTE>VERS\", -- Fel-Scarred (archon.gg / raid & m+)\n                [35] = t .. s .. \"CRIT>ILVL>MAST>HASTE>VERS\", -- Aldrachi Reaver (wowhead.com / supplement)\n            },\n            \n            VENGEANCE = \n            {\n                [34] = t .. b .. \"ILVL>HASTE>CRIT>MAST>VERS\", -- Fel-Scarred (archon.gg / raid & m+)\n                [35] = t .. s .. \"ILVL>HASTE>CRIT>VERS>MAST\",-- Aldrachi Reaver (wowhead.com / supplement)\n            },\n        },\n        DRUID = \n        {\n            BALANCE = \n            {\n                [21] = e, -- Druid of the Claw\n                [22] = e, -- Wildstalker\n                [23] = t .. r .. \"ILVL>MAST>HASTE>CRIT>VERS\", -- Keeper of the Grove (archon.gg / raid)\n                [24] = t .. m .. \"ILVL>MAST>H