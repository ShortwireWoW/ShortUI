["custom"] = "function(e, ...)\n    if ... then\n        local num, height = ... -- sent by bosses aura\n        aura_env.groupregion = aura_env.groupregion or WeakAuras.GetData(\"M+ Dynamic Group\")\n        local spacing = aura_env.groupregion and aura_env.groupregion.space or 3 -- spacing in dynamic group\n        local inforegion = WeakAuras.GetRegion(\"M+ KeyInfo\")\n        local info = inforegion and inforegion.height or 16 -- keyinfo height, else 16 as base\n        local timerregion = WeakAuras.GetRegion(\"M+ Timer Bar\")\n        local timerheight = timerregion and timerregion.height or 24 -- timer bar region height, else 24 as base\n        local forcesregion = WeakAuras.GetRegion(\"M+ Enemy Forces Bar\")\n        local forces = forcesregion and forcesregion.height or 24 -- enemy forces region height, else 24 as base\n        local timer = timerheight+spacing -- bar height + text at the top + spacing\n        local bosses = num*(height+spacing) -- num of states times its bar height+spacing\n        if aura_env.region then\n            aura_env.region:SetRegionHeight(forces+timer+bosses+info)\n            aura_env.region:SetOffset(0, -2)          \n            return true\n        end\n    end\nend",
